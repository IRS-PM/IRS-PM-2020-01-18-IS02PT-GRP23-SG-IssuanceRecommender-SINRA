package com.iss_mr.integrated_shield_plan_master;

import java.lang.Number;

rule "PolicyReasoning"
	dialect "mvel"
	ruleflow-group "policyreasoning"
	when
		app : Application( issuer == "AIA" )
	then
		Policy newp = new Policy();
		newp.setName( "AIA HealthShield Gold Max A" );
		newp.setBenefit( "As Charged" );
		newp.setInsurer( "AIA" );
		insert( newp );
		app.setMatchedPolicy( newp );
end
